[user]
  name = Magnus GÃ¶rlitz
  email = magnus.goerlitz@makandra.de

[alias]
  c = checkout
  c = commit
  s = status
  b = branch

[color]
    ui = auto
  [color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
  [color "diff"]
    meta = yellow
    frag = magenta
    old = red
    new = green
    whitespace = red reverse
  [color "status"]
    added = yellow
    changed = green
    untracked = cyan

[core]
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
  editor=nvim
  excludesfile = /home/magnus/.gitignore
[push]
  default = current
[pull]
[core]
  excludesfile = /home/your_name/.gitignore
